{
  unstable,
  self,
  ...
}: {pkgs, ...}: {
  config = {
    nixpkgs.allowedUnfree = ["discord" "google-chrome"];

    activeProfiles = ["browsing"];

    dconf.enable = true;

    home.packages = let
      p = pkgs;
      s = self.packages.x86_64-linux;
    in [
      p.keybase-gui
      p.minikube
      p.freerdp
      p.ipfs
    ];

    programs.htop = {
      settings = {
        detailed_cpu_time = true;
      };
    };

    xsession.windowManager.awesome.autostart = [
      "${pkgs.blueman}/bin/blueman-applet"
      "${pkgs.networkmanagerapplet}/bin/nm-applet"
    ];

    systemd.user.services = {
      keybase-gui = {
        Unit = {
          Description = "Keybase GUI";
          Requires = ["keybase.service" "kbfs.service"];
          After = ["keybase.service" "kbfs.service"];
        };
        Service = {
          ExecStart = "${pkgs.keybase-gui}/share/keybase/Keybase";
          PrivateTmp = true;
          # Slice      = "keybase.slice";
        };
      };
    };
  };
}
